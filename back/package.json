{
  "name": "graphql-back",
  "description": "Koa 2 RESTful API boilerplate",
  "version": "0.0.1",
  "engines": {
    "node": ">=9.0.0"
  },
  "bin": {
    "mainrest": ".dist/bundle.js"
  },
  "keywords": [
    "koa2",
    "graphql",
    "api",
    "cassandra",
    "async",
    "es7"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nczkevin/rest-koa.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nczkevin/rest-koa/issues"
  },
  "homepage": "https://github.com/nczkevin/rest-koa#readme",
  "main": "server/index.js",
  "scripts": {
    "fixtures:users": "babel-node fixtures/users.js",
    "fixtures:contacts": "babel-node fixtures/contacts.js",
    "service:nats-server": "cd ~/GoglandProjects/bin && ./gnatsd -DV -config ../nats.00.conf",
    "db:elasticsearch": "cd ~/install/elasticsearch-6.0.0/bin && ./elasticsearch",
    "db:kibana": "cd ~/install/kibana-6.0.0-linux-x86_64/bin && ./kibana",
    "db:cassandra": "service cassandra start",
    "start:prod": "NODE_ENV=production nodemon server/graphql/ --exec babel-node",
    "start:dev": "NODE_ENV=development nodemon server/server.js --exec babel-node",
    "start:static": "NODE_ENV=development nodemon server/server-static.js --exec babel-node",
    "build": "rm -rf dist && babel server -d dist -s",
    "build:compact": "rm -rf dist && mkdir dist && babel server --out-file ./dist/server.js",
    "bundle": "rm -rf bundle && ./node_modules/.bin/webpack",
    "bundle:start": "nodemon bundle/bundle.js",
    "lint": "eslint --fix ./server",
    "test": "npm run mocha",
    "test:connection": "mocha -g 'Basic Connection' --require babel-register --require babel-polyfill --timeout 1500 || true",
    "test:orm": "mocha -g 'ORM' --compilers js:babel-register --require babel-polyfill --timeout 1500 || true",
    "prod": "npm run build && node dist/",
    "test:full": "./node_modules/.bin/mocha -g 'validator' --compilers js:babel-register --require babel-polyfill --timeout 1500 || true",
    "test:lib": "./node_modules/.bin/mocha server/lib/commons/test/* --compilers js:babel-register --require babel-polyfill ignore-styles --timeout 1500 || true",
    "test:nats-server": "./node_modules/.bin/mocha -g 'NATS' --compilers js:babel-register --require babel-polyfill --timeout 1500 || true",
    "test:graphql": "./node_modules/.bin/mocha test/graphql/* --compilers js:babel-register --require babel-polyfill ignore-styles --timeout 1500 || true",
    "flow:check": "./node_modules/.bin/flow check"
  },
  "dependencies": {
    "//": "do not upgrade merge-graphql-schemas 0.11.7 or 0.13 avoid 0.12.3",
    "apollo-server-koa": "^1.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cassandra-driver": "^3.4.1",
    "dataloader": "^1.4.0",
    "dotenv": "^5.0.1",
    "elasticsearch": "^14.1.0",
    "eventemitter2": "^5.0.1",
    "fs": "^0.0.1-security",
    "graphql": "0.13.1",
    "graphql-iso-date": "^3.5.0",
    "graphql-tools": "^2.21.0",
    "graphql-type-json": "^0.2.0",
    "grpc": "^1.11.0-pre2",
    "jsonwebtoken": "^8.1.1",
    "kcors": "next",
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-jwt": "^3.3.0",
    "koa-logger": "^3.1.0",
    "koa-mount": "^3.0.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-session2": "^2.2.5",
    "koa-sslify": "^2.1.2",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5",
    "merge-graphql-schemas": "^1.4.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "nats": "^0.7.29",
    "pm2": "^2.10.2",
    "readable-stream": "^2.3.4",
    "resize-img": "^1.1.2",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dynamic-import": "^2.0.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "casual": "^1.5.19",
    "chai": "^4.1.2",
    "core-decorators": "^0.20.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-cassandra": "^2.2.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.66.0",
    "loader-utils": "^1.1.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "nyc": "^11.4.1",
    "restc": "^0.3.2",
    "should": "^13.2.1",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0"
  }
}
