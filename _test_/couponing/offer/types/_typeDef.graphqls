type Offer {
  id: ID!
  userId: ID!
  name: String
  description: String
  provider: Provider # todo load bigdata
  brand: String # todo  import from admin
  isEanFromBrand: Boolean
  eanOrigin: eanOriginEnum
  categoryId: String # todo OK included in main schema
  discount: Discount
  eanList: [EAN]!
  quantityMinToSend: Int!
  quantityMaxToSend: Int!
